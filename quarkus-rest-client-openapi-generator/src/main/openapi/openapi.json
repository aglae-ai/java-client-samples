                "schema": {
                  "type": "object"
                }
              }
            }
          },
          "404": {
            "description": "No offer found"
          },
          "default": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/MatchingOutputDto"
                  }
                }
              }
            }
          }
        }
      }
    }
